{
  "name": "band-protocol-frontend-assignment",
  "version": "1.0.0",
  "description": "",
  "web": "dist/index.html",
  "targets": {
    "web": {
      "source": "src/index.html"
    }
  },
  "scripts": {
    "start": "parcel src/index.html",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "NODE_ENV=production npm run css-build && parcel build --target dist",
    "css-build": "npx tailwindcss -i ./src/input.css -o ./dist/output.css",
    "watch": "NODE_ENV=test parcel watch src/index.html && npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-elm": "^2.6.2",
    "concurrently": "^7.2.2",
    "parcel": "^2.6.2",
    "tailwindcss": "^3.1.6"
  },
  "dependencies": {
    "elm": "^0.19.1-5"
  }
}
